// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["fullTextSearch","fullTextIndex"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}
model Book{
  id Int @id @default(autoincrement())
  title String
  Author String
  ISBN BigInt
  Year Int
  Publisher String
}
