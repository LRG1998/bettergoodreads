// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.sqlite"
}
model Book{
  id Int @id @default(autoincrement())
  ISBN Int
  title String
  Author String
  Year Int
  Publisher String
}